generator client {
  provider = "prisma-client-js"
  output   = "./generated/client_ragnarok"
}

datasource db {
  provider = "mysql"
  url      = env("DB_RAGNAROK_URL")
}

model annunci {
  annunci_id Int    @id @default(autoincrement())
  titolo     String @db.Text
  corpo      String @db.Text
  autore     String @db.Text
  tag        String @db.Text
  data       String @db.Text
  cover      String @db.Text
}
